with "qt_core";
with "league";
with "uim_config";

project Proto is

   for Languages use ("Ada");
   for Library_Name use "uim_proto";
   for Library_Kind use "dynamic";

   for Source_Dirs use ("../source/proto/", "../source/proto/.amoc");
   for Library_Dir use "../.libs";
   for Object_Dir  use "../.objs";

   package Compiler is
      for Default_Switches ("Ada") use UIM_Config.Default_Switches;
   end Compiler;

   package Builder is
      for Global_Configuration_Pragmas use "qtada.adc";
   end Builder;

end Proto;
